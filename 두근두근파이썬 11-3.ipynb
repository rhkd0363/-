    "alp={}\n",
    "filename = input(\"입력 파일 이름:\")\n",
    "infile = open(filename,\"r\")\n",
    "lines = infile.read()\n",
    "infile.close()\n",
    "\n",
    "for line in lines:\n",
    "    for ch in line:\n",
    "        if ch in alp:\n",
    "            alp[ch] += 1\n",
    "        else:\n",
    "            alp[ch] = 1\n",
    "print(alp)"
   ]
  }
